    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "prettier": "^2.8.8",
    "vite-plugin-svgr": "^3.2.0",
    "vite": "^4.4.9"
